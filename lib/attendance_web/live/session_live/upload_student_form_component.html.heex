<div>
  <h2><%= @title %></h2>

  <.form
    let={f}
    for={@changeset}
    id="student-form"
    phx-target={@myself}
    phx-change="validate"
    phx-submit="save">

    <%= hidden_input f, :first_name %>

    <div>
    <%= live_file_input @uploads.csv_file %>
  </div>

    <div>
      <%= submit "Save", phx_disable_with: "Saving..." %>
    </div>
  </.form>
</div>
